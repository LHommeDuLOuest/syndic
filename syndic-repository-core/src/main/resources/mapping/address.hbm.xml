<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- @author LYES KHERBICHE -->
<hibernate-mapping>
    <class name="fr.upond.syndic.repo.model.common.Address" table="ADDRESS">
        <composite-id>
            <key-property name="numAdress"       column="NUMADRESS"/>
            <key-property name="typeAddress"     column="TYPEADDRESS"/>
            <key-property name="street"          column="STREET"/>
            <key-property name="city"            column="CITY"/>
            <key-property name="zipCode"         column="ZIPCODE"/>
            <key-property name="country"         column="COUNTRY"/>
        </composite-id >
        <property name="placeName" column="PLACENAME" />
    </class>
</hibernate-mapping>
