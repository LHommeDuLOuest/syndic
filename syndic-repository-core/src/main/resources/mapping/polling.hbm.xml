<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- @author LYES KHERBICHE -->
<hibernate-mapping>
   <class name="fr.upond.syndic.repo.model.common.Polling" table="POLLING">
		<id name="id" type="int" column="ID">
         <generator class="native"/>
        </id>
		<property name="question" column="QUESTION" />
        <property name="pour" column="POUR" />
        <property name="contre" column="CONTRE" />
        <property name="abstention" column="ABSTENTION" />
   </class>
</hibernate-mapping>