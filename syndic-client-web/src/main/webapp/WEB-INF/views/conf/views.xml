<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans SYSTEM "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

    
    <bean id="welcomePage" class="org.springframework.web.servlet.view.JstlView">
		<property name="url">
			<value>/index.jsp</value>
		</property>
	</bean>
	<bean id="addUserPage" class="org.springframework.web.servlet.view.JstlView">
		<property name="url">
			<value>/WEB-INF/views/pages/addUser.jsp</value>
		</property>
	</bean>
	<bean id="addEventPage" class="org.springframework.web.servlet.view.JstlView">
		<property name="url">
			<value>/WEB-INF/views/pages/addEvent.jsp</value>
		</property>
	</bean>
    

</beans>